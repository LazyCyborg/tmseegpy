{
  "name": "tmseegpy-gui",
  "version": "0.1.8",
  "private": true,
  "homepage": "./",
  "author": "LazyCyborg <hjarneko@gmail.com>",
  "description": "GUI for the tmseegpy Toolbox",
  "main": "public/electron.js",
  "files": [
    "build/**/*",
    "public/**/*"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-table": "^8.11.3",
    "axios": "^1.6.5",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.1.7",
    "framer-motion": "^10.17.9",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.2",
    "node-fetch": "^2.7.0",
    "tailwindcss": "^3.4.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@electron/notarize": "^2.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "rimraf": "^5.0.5",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp public/electron.js build/ && cp public/preload.js build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:linux": "npm run build && electron-builder --linux"
  },
  "build": {
    "appId": "com.lazycyborg.tmseegpy-gui",
    "productName": "TMSeegpy GUI",
    "copyright": "Copyright Â© 2024 LazyCyborg",
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "files": [
      "build/**/*",
      "public/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Science",
      "icon": "assets/icon.png"
    },
    "publish": {
      "provider": "github",
      "owner": "LazyCyborg",
      "repo": "tmseegpy",
      "private": false
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
